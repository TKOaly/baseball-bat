{
  "name": "@bbat/backend",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start:dev": "node --watch -r esbuild-register -r ../../dotenv.ts --inspect=0.0.0.0 ./src/app.ts",
    "start": "node -r tsconfig-paths/register ./src/app.js",
    "test:unit": "NODE_ENV=testing TZ=Europe/London ASSET_PATH=./assets node --dns-result-order=ipv4first -r esbuild-register -r ../../dotenv.ts --test",
    "test:unit:all": "pnpm test:unit tests/unit/*.test.ts",
    "test:e2e": "NODE_OPTIONS='--dns-result-order=ipv4first -r esbuild-register -r ../../dotenv.ts' playwright test",
    "migrate": "node-pg-migrate -d POSTGRES_CONNECTION_STRING up",
    "build": "tsc",
    "lint": "eslint src && prettier --check src",
    "pre-commit": "lint-staged"
  },
  "main": "./build/app.js",
  "files": [
    "./tsconfig.json",
    "./build/src/*",
    "./assets/*",
    "./migrations/*",
    "./templates/*"
  ],
  "dependencies": {
    "@bbat/common": "workspace:*",
    "@types/busboy": "^1.5.3",
    "@types/ejs": "^3.1.5",
    "@types/mjml": "^4.7.4",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.8",
    "@types/xml2js": "^0.4.14",
    "@vendia/serverless-express": "^4.12.6",
    "@xmldom/xmldom": "^0.8.10",
    "axios": "^1.6.8",
    "base64url": "^3.0.1",
    "bullmq": "^5.4.2",
    "busboy": "^1.6.0",
    "canvas": "^2.11.2",
    "class-variance-authority": "^0.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "ejs": "^3.1.9",
    "eslint-config-prettier": "^9.1.0",
    "eventemitter2": "^6.4.9",
    "express": "^4.19.2",
    "finnish-bank-utils": "git+https://github.com/vkomulai/finnish-bank-utils.git#b660a75",
    "flowbite": "^2.3.0",
    "formik": "^2.4.5",
    "fp-ts": "^2.16.2",
    "helmet": "^7.1.0",
    "husky": "^9.0.11",
    "i18next": "^23.10.1",
    "immer": "^10.0.3",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19",
    "jsbarcode": "^3.11.6",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3",
    "mjml": "^4.15.3",
    "mjml-browser": "^4.15.3",
    "monocle-ts": "^2.3.13",
    "multer": "1.4.5-lts.1",
    "newtype-ts": "^0.3.5",
    "node-cron": "^3.0.3",
    "node-pg-migrate": "^6.2.2",
    "nodemailer": "^6.9.12",
    "openid-client": "^5.6.5",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "pg-types": "^4.0.2",
    "puppeteer": "^22.4.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "remeda": "^1.47.0",
    "sql-template-strings": "^2.2.2",
    "stripe": "^14.20.0",
    "typedi": "^0.10.0",
    "typera-common": "^2.4.3",
    "typera-express": "^2.4.4",
    "uid": "^2.0.2",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2",
    "xml2js-xpath": "^0.13.0"
