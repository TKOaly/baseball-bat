    "dotenv": "^16.0.0",
    "ejs": "^3.1.7",
    "eslint-config-prettier": "^9.0.0",
    "express": "^4.17.2",
    "finnish-bank-utils": "git+https://github.com/vkomulai/finnish-bank-utils.git#b660a75",
    "flowbite": "^1.4.1",
    "formik": "^2.2.9",
    "fp-ts": "^2.11.8",
    "helmet": "^5.0.2",
    "husky": "^8.0.3",
    "i18next": "^21.9.1",
    "immer": "^10.0.2",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.19",
    "jsbarcode": "^3.11.5",
    "jsdom": "^20.0.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^14.0.1",
    "mjml": "^4.12.0",
