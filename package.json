    "prettier": "^3.0.3",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "tailwindcss": "^3.0.23",
    "timers-browserify": "^2.0.11",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.5",
    "vm-browserify": "^1.1.2",
    "yargs-parser": "^21.1.1"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": [
      "STRIPE_PUBLIC_KEY",
      "BACKEND_URL",
      "APP_URL",
      "NODE_ENV"
    ]
  },
  "alias": {
    "process": false
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "ava": {
    "failWithoutAssertions": false,
    "files": [
      "tests/unit/**/*"
    ],
    "typescript": {
      "rewritePaths": {
        "tests/": "build/tests/"
      },
      "compile": "tsc"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{yml,md,html,json}": "prettier --write"
  }
}
