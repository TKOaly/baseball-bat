    "@types/styled-components": "^5.1.23",
    "aws-cdk": "^2.14.0",
    "@aws-cdk/core": "1.146.0",
    "@aws-cdk/aws-lambda": "1.146.0",
    "@aws-cdk/aws-ec2": "1.146.0",
    "@aws-cdk/aws-apigateway": "1.146.0",
