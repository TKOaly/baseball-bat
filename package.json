    "@types/puppeteer": "^7.0.4",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "cypress": "^11.2.0",
    "earl": "^1.1.0",
