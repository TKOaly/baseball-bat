{
  "name": "bbat",
  "license": "MIT",
  "scripts": {
    "start-server:dev": "ts-node-dev backend/app.ts",
    "start-frontend:watch": "NODE_ENV=development parcel watch ./public/index.html --dist-dir ./web-dist",
    "start:dev": "concurrently \"yarn start-server:dev\" \"yarn start-frontend:watch\"",
    "start": "node ./build/backend/app.js",
    "build:server": "tsc",
    "build:frontend": "parcel build --no-scope-hoist ./public/index.html --dist-dir ./web-dist",
    "migrate": "node-pg-migrate -d POSTGRES_CONNECTION_STRING up",
    "test": "NODE_ENV=testing ava -s --no-worker-threads --timeout=30s",
    "coverage": "c8 yarn test",
    "prettier": "prettier --write --cache './**/*.{js,jsx,ts,tsx,css,mjml}'",
    "prettier:check": "prettier --check --cache './**/*.{js,jsx,ts,tsx,css,mjml}'",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.1",
