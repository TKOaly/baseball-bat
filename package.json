    "axios": "^0.26.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.17.2",
    "fp-ts": "^2.11.8",
    "helmet": "^5.0.2",
    "io-ts": "^2.2.16",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remeda": "^0.0.32",
    "sql-template-strings": "^2.2.2",
    "stripe": "^8.202.0",
    "styled-components": "^5.3.3",
